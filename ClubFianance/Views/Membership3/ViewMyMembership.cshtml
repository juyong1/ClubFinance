@model IEnumerable<ClubFinance.Models.Membership3>

@{
    ViewBag.Title = "Welcome! You Joined New Membership!";
}

<h2>Welcome! You Joined New Membership!</h2>

<table class="table">
    <tr>
        <th>
            Club Name
        </th>
        <th>
            Club NickName
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Option)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegistrationDate)
        </th>
       <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @ViewBag.ClubName
        </td>
        <td>
            @ViewBag.ClubNickName
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Option)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegistrationDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
